pipeline {
    agent any
    stages{
        stage('Install dependencies') {
            steps{
                echo 'install'
            }
        }
        stage('Run unit test') {
            steps{
                echo 'test'
            }
        }
        stage('Run deploy') {
            steps{
                echo "Se realizo el deploy with codepipeline"
            }
        }
    }
}